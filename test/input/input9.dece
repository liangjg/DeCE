echo "reading inelastic scattering cross sections"
multiread 3 51 91 "data/CoHLevelExcite1.dat"

echo "adding all subsections"
make4

echo "saving MF4 in MF3"
copy 3 4 3

echo "applying reversed-Fermi function"
applyfunc3 3 4 -0.5 8.0 2.0

extract 3 4

echo "re-adjusting partial cross sections"
readjust 4

echo "making MF4 again by adjusted partial data"
make4

extract 3 3
extract 3 4

quit
