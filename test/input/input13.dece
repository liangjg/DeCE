echo "reading elastic scattering cross section"
read 3 2  "data/CoHCrossSection.dat"

echo "reading elastic scattering angular distribution in MF4"
angdist 4 2  "data/CoHAngularDistribution.dat"

echo "Legendre data generated by least-squares fitting"
echo "to the differential cross sections"
extract 4 2

echo "setting angle increment"
set AngleStep 15

echo "differential data at equi-anglar points"
table 4 2

quit
